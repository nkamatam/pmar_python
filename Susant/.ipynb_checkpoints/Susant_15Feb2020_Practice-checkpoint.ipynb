    "n=6\n",
    "for i in range (0,n,1):\n",
    "    if i==0:\n",
    "        print(\" \"*(n+1)+\"*\")\n",
    "    else:\n",
    "        print(\" \"*(n-i)+\"*\"+\" \"*(2*i+1)+\"*\")\n",
    "for i in range (n,0,-1):\n",
    "    if i==1:        \n",
    "        print(\" \"*(n+1)+\"*\")\n",
    "    else:\n",
    "        print(\" \"*(n-i+1)+\"*\"+\" \"*(2*i-1)+\"*\")"
